class Solution {
public:
    bool isValid(string s) {
        stack<char>ss;

    for(char ch:s){
        if(ch=='{'||ch=='('||ch=='['){
            ss.push(ch);
        }
        else{
            if(!ss.empty()){
                if(ch=='}'){
                if(ss.top()=='{'){
                    ss.pop();
                }
                else{
                    return false;
                }
            }
            else if(ch==')'){
                if(ss.top()=='('){
                    ss.pop();
                }
                else{
                    return false;
                }
            }
            else if(ch==']'){
                if(ss.top()=='['){
                    ss.pop();
                }
                else{
                    return false;
                }
            }
            }
            else{
                return false;
            }
        }
        
    }
    if(ss.empty()){
        return true;
    }
    return false;
    }
};
